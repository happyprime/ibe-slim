{
	"name": "@automattic/isolated-block-editor",
	"version": "2.29.0",
	"description": "Repackages Gutenberg's editor playground as multi-instance editor.",
	"main": "build/index.js",
	"module": "build-module/index.js",
	"types": "build-types",
	"homepage": "https://github.com/Automattic/isolated-block-editor/blob/trunk/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/isolated-block-editor.git"
	},
	"bugs": {
		"url": "https://github.com/Automattic/isolated-block-editor/issues"
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "BUILD_ENV=cjs babel src --out-dir build --source-maps --ignore 'src/**/__tests__/*.js' --copy-files --no-copy-ignored && rm -rf build/browser",
		"clean": "rm -rf build",
		"format": "wp-scripts format",
		"lint:js": "wp-scripts lint-js"
	},
	"sideEffects": [
		"*.css",
		"*.scss"
	],
	"author": "Automattic",
	"license": "GPL-2.0-or-later",
	"dependencies": {
		"@happyprime/wp-super-unlock": "github:happyprime/wp-super-unlock",
		"classnames": "^2.5.1",
		"redux-undo": "^1.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.24.8",
		"@babel/core": "^7.25.2",
		"@babel/plugin-transform-runtime": "^7.24.7",
		"@babel/preset-env": "^7.25.3",
		"@babel/preset-react": "^7.24.7",
		"@wordpress/babel-preset-default": "^8.5.0",
		"@wordpress/prettier-config": "^4.5.0",
		"@wordpress/scripts": "^28.5.0",
		"prettier": "npm:wp-prettier@3.0.3",
		"terser-webpack-plugin": "^5.3.10"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
