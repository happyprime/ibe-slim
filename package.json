{
	"name": "@happyprime/ibe-slim",
	"version": "3.0.0",
	"description": "A fork of @automattic/isolated-block-editor. Unsupported.",
	"main": "build/index.js",
	"homepage": "https://github.com/happyprime/ibe-slim/blob/trunk/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/happyprime/ibe-slim.git"
	},
	"bugs": {
		"url": "https://github.com/happyprime/ibe-slim/issues"
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "BUILD_ENV=cjs babel src --out-dir build --source-maps --ignore 'src/**/__tests__/*.js' --copy-files --no-copy-ignored && rm -rf build/browser",
		"clean": "rm -rf build",
		"format": "wp-scripts format",
		"lint:js": "wp-scripts lint-js"
	},
	"sideEffects": [
		"*.css",
		"*.scss"
	],
	"author": "Happy Prime",
	"license": "GPL-2.0-or-later",
	"dependencies": {
		"@happyprime/wp-super-unlock": "github:happyprime/wp-super-unlock",
		"classnames": "^2.5.1",
		"redux-undo": "^1.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.24.8",
		"@babel/core": "^7.25.2",
		"@babel/plugin-transform-runtime": "^7.24.7",
		"@babel/preset-env": "^7.25.3",
		"@babel/preset-react": "^7.24.7",
		"@happyprime/eslint-config": "^0.0.8",
		"@wordpress/babel-preset-default": "^8.5.0",
		"terser-webpack-plugin": "^5.3.10"
	}
}
